{
  "name": "food-calorie-pwa",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "workers": "wrangler dev --env development",
    "workers:deploy": "wrangler deploy",
    "pages:deploy": "npm run build && wrangler pages deploy dist",
    "deploy": "npm run pages:deploy && npm run workers:deploy",
    "deploy:local": "bash scripts/deploy.sh",
    "deploy:local:dev": "bash scripts/deploy.sh dev"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^4.4.0",
    "wrangler": "^3.12.0"
  }
}
